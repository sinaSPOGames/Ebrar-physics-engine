[memory]

limits/multithreaded_server/rid_pool_prealloc=500
limits/message_queue/max_size_kb=7040
limits/command_queue/multithreading_queue_size_kb=2244

[physics]

3d/smooth_trimesh_collision=true
common/physics_fps=200
common/physics_jitter_fix=0.0
3d/physics_engine="Bullet"
3d/active_soft_world=false
common/physics_interpolation=true

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=3
quality/directional_shadow/size=512
quality/shadow_atlas/size=256
quality/shadow_atlas/quadrant_1_subdiv=1
quality/shadow_atlas/quadrant_2_subdiv=1
quality/shadow_atlas/quadrant_3_subdiv=1
quality/shadows/filter_mode=2
quality/shading/force_lambert_over_burley=true
quality/shading/force_blinn_over_ggx=true
quality/spatial_partitioning/render_tree_balance=1.0
quality/spatial_partitioning/use_bvh=false
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
